{
    "name": "xqkeji/xq-com-bootstrap",
    "description": "新齐通用后台管理页面系统的使用的bootstrap的js、css等前端程序的composer包。",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "xqkeji\\composer\\bootstrap\\": ""
        }
    },
    "authors": [
        {
            "name": "xqkeji.cn"
        }
    ],
    "extra": {
        "post-package-install":[
            {
                "cmd":["xqkeji\\composer\\Asset","copyDirs"],
                "param":[{
                    "vendor/xqkeji/xq-com-bootstrap/src/":"resource/bootstrap/"
                }]
            }
        ],
        "post-package-update":[
            {
                "cmd":["xqkeji\\composer\\Asset","rmDirs"],
                "param":[{
                    "vendor/xqkeji/xq-com-bootstrap/src/":"resource/bootstrap/"
                }]
            },
            {
                "cmd":["xqkeji\\composer\\Asset","copyDirs"],
                "param":[{
                    "vendor/xqkeji/xq-com-bootstrap/src/":"resource/bootstrap/"
                }]
            }
        ],
        "post-package-uninstall":[
            {
                "cmd":["xqkeji\\composer\\Asset","rmDirs"],
                "param":[{
                    "vendor/xqkeji/xq-com-bootstrap/src/":"resource/bootstrap/"
                }]
            }
        ]
    }
}
